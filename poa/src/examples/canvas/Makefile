	@$(QMAKE) canvas.pro

dist: 
	@mkdir -p .tmp/canvas && $(COPY_FILE) --parents $(SOURCES) $(HEADERS) $(FORMS) $(DIST) .tmp/canvas/ && ( cd `dirname .tmp/canvas` && $(TAR) canvas.tar canvas && $(GZIP) canvas.tar ) && $(MOVE) `dirname .tmp/canvas`/canvas.tar.gz . && $(DEL_DIR) .tmp/canvas

