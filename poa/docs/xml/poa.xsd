<?xml version="1.0" encoding="UTF-8"?><xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">	<xs:element name="project">		<xs:annotation>			<xs:documentation>POA-Projekt</xs:documentation>		</xs:annotation>		<xs:complexType>			<xs:sequence>				<xs:element name="model">					<xs:annotation>						<xs:documentation>Datenmodell</xs:documentation>					</xs:annotation>					<xs:complexType>						<xs:sequence>							<xs:element name="blocks">								<xs:annotation>									<xs:documentation>Blöcke</xs:documentation>								</xs:annotation>								<xs:complexType>									<xs:sequence>										<xs:element name="model-item" maxOccurs="unbounded">											<xs:annotation>												<xs:documentation>Datenmodell, Logische Blöcke</xs:documentation>											</xs:annotation>											<xs:complexType>												<xs:sequence>													<xs:element name="pin" maxOccurs="unbounded">														<xs:annotation>															<xs:documentation>Pins</xs:documentation>														</xs:annotation>														<xs:complexType>															<xs:attribute name="address" type="xs:integer" use="required"/>															<xs:attribute name="bits" type="xs:integer" use="required"/>															<xs:attribute name="position" type="xs:integer" use="required"/>															<xs:attribute name="type" use="required">																<xs:simpleType>																	<xs:restriction base="xs:string">																		<xs:enumeration value="input"/>																		<xs:enumeration value="output"/>																		<xs:enumeration value="episodic"/>																	</xs:restriction>																</xs:simpleType>															</xs:attribute>															<xs:attribute name="id" type="xs:integer" use="required"/>															<xs:attribute name="name" type="xs:string" use="required"/>														</xs:complexType>													</xs:element>												</xs:sequence>												<xs:attribute name="offset" type="xs:integer" use="required"/>												<xs:attribute name="desc" type="xs:string" use="required"/>												<xs:attribute name="hasOutputPins" type="xs:boolean" use="required"/>												<xs:attribute name="hasInputPins" type="xs:boolean" use="required"/>												<xs:attribute name="clock" type="xs:integer" use="required"/>												<xs:attribute name="runtime" type="xs:integer" use="required"/>												<xs:attribute name="hasEpisodicPins" type="xs:boolean" use="required"/>												<xs:attribute name="type" type="xs:string" use="required"/>												<xs:attribute name="id" type="xs:string" use="required"/>												<xs:attribute name="name" type="xs:string" use="required"/>												<xs:attribute name="auto-offset" type="xs:boolean" use="required"/>												<xs:attribute name="hasRuntime" type="xs:boolean" use="required"/>												<xs:attribute name="block-type" use="required">													<xs:simpleType>														<xs:restriction base="xs:string">															<xs:enumeration value="block"/>															<xs:enumeration value="cpu"/>														</xs:restriction>													</xs:simpleType>												</xs:attribute>												<xs:attribute name="cpuid" type="xs:integer"/>												<xs:attribute name="auto-runtime" type="xs:boolean"/>											</xs:complexType>										</xs:element>									</xs:sequence>								</xs:complexType>							</xs:element>						</xs:sequence>					</xs:complexType>				</xs:element>				<xs:element name="views">					<xs:annotation>						<xs:documentation>Views</xs:documentation>					</xs:annotation>					<xs:complexType>						<xs:sequence>							<xs:element name="view">								<xs:annotation>									<xs:documentation>Viewinstanz</xs:documentation>								</xs:annotation>								<xs:complexType>									<xs:choice maxOccurs="unbounded">										<xs:element name="view-item">											<xs:annotation>												<xs:documentation>Erweitert das Model um Informationen zur graphischen Darstellung</xs:documentation>											</xs:annotation>											<xs:complexType>												<xs:attribute name="x" type="xs:integer" use="required"/>												<xs:attribute name="y" type="xs:integer" use="required"/>												<xs:attribute name="model-id" type="xs:integer" use="required"/>											</xs:complexType>										</xs:element>										<xs:element name="connector-view">											<xs:annotation>												<xs:documentation>Repräsentiert eine Verbindung zwischen zwei Blöcken</xs:documentation>											</xs:annotation>											<xs:complexType>												<xs:sequence>													<xs:element name="points">														<xs:annotation>															<xs:documentation>Knickpunkte</xs:documentation>														</xs:annotation>														<xs:complexType>															<xs:sequence>																<xs:element name="point" maxOccurs="unbounded">																	<xs:annotation>																		<xs:documentation>Knickpunkt</xs:documentation>																	</xs:annotation>																	<xs:complexType>																		<xs:attribute name="x" type="xs:integer" use="required"/>																		<xs:attribute name="y" type="xs:integer" use="required"/>																	</xs:complexType>																</xs:element>															</xs:sequence>														</xs:complexType>													</xs:element>												</xs:sequence>												<xs:attribute name="target-pin" type="xs:integer" use="required"/>												<xs:attribute name="target-block" type="xs:integer" use="required"/>												<xs:attribute name="source-pin" type="xs:integer" use="required"/>												<xs:attribute name="source-block" type="xs:integer" use="required"/>											</xs:complexType>										</xs:element>										<xs:element name="palette-view">											<xs:annotation>												<xs:documentation>Repräsentiert die Palette</xs:documentation>											</xs:annotation>											<xs:complexType>												<xs:sequence>													<xs:element name="mapping" maxOccurs="unbounded">														<xs:annotation>															<xs:documentation>Mapping: Farbe - Taktrate</xs:documentation>														</xs:annotation>														<xs:complexType>															<xs:attribute name="ns" type="xs:integer" use="required"/>															<xs:attribute name="pal-entry" type="xs:integer" use="required"/>														</xs:complexType>													</xs:element>												</xs:sequence>												<xs:attribute name="last-pal-entry" type="xs:integer" use="required"/>												<xs:attribute name="x" type="xs:integer" use="required"/>												<xs:attribute name="y" type="xs:integer" use="required"/>												<xs:attribute name="name" type="xs:string" use="required"/>											</xs:complexType>										</xs:element>									</xs:choice>									<xs:attribute name="name" type="xs:string" use="required"/>								</xs:complexType>							</xs:element>						</xs:sequence>					</xs:complexType>				</xs:element>			</xs:sequence>		</xs:complexType>	</xs:element></xs:schema>